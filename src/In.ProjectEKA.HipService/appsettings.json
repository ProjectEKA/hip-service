{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },{
        "Name": "File", "Args": { "path": "../../logs/log.log", "rollingInterval": "Day" }
      }
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection":
    "Host=db;Port=5432;Username=postgres;Password=welcome;Database=hipservice;"
  },
  "OtpService": {
    "BaseUrl": "http://otp",
    "OffsetInMinutes": 5
  },
  "rabbit": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "test",
    "Password": "test"
  },
  "dataFlow": {
    "dataSizeLimitInMbs": 5
  },
  "hip": {
    "url": "http://hip:8000"
  },
  "Gateway": {
    "url": "http://localhost:8081",
    "timeout": 3000,
    "clientId": "10000005",
    "clientSecret": "95ca9a9d-1473-4172-9d87-0fd4d87ee1d6",
  },
  "BackgroundJobs": {
    "cancellationCheckInterval": 5
  }
}
